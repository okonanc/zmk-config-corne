 #include "corne.dtsi"

 &usbd {
    status = "okay";
    cdc_acm_uart: cdc_acm_uart {
        compatible = "zephyr,cdc-acm-uart";
    };
};

 &kscan0 {
    compatible = "zmk,kscan-gpio-direct";
    label = "KSCAN";
    input-gpios
    = <&gpio1 6 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>
    ;
 };